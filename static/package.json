    "@stryker-mutator/core": "^9.5.1",
    "@stryker-mutator/vitest-runner": "^9.5.1",
    "@tailwindcss/cli": "^4.1.18",
    "@tsconfig/node24": "^24.0.4",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^25.2.1",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
